{
  "name": "@thewhodidthis/life",
  "version": "1.0.3",
  "description": "Just another game of life runner",
  "main": "index",
  "module": "index.mjs",
  "homepage": "https://github.com/thewhodidthis/life#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewhodidthis/life.git"
  },
  "bugs": {
    "url": "https://github.com/thewhodidthis/life/issues"
  },
  "scripts": {
    "pretest": "eslint test.js",
    "test": "node test",
    "prestart": "eslint index.mjs",
    "start": "rollup -f cjs -e @thewhodidthis/otto --no-interop index.mjs > index.js",
    "preexample": "eslint example/index.es",
    "example": "cd example; rollup -c node:x -f iife index.es | buble > index.js",
    "postexample": "open example/index.html"
  },
  "eslintConfig": {
    "extends": "@thewhodidthis"
  },
  "engines": {
    "node": ">=6.4.0",
    "npm": ">=3.10.3"
  },
  "dependencies": {
    "@thewhodidthis/otto": "github:thewhodidthis/otto"
  },
  "devDependencies": {
    "@thewhodidthis/eslint-config": "latest",
    "buble": "latest",
    "eslint": "latest",
    "rollup": "latest",
    "rollup-config-x": "latest",
    "tapeless": "latest"
  },
  "keyowrds": [
    "cellular-automata",
    "game-of-life"
  ],
  "author": "sChzs BkgNns <sotirios.bakagiannis@gmail.com> (https://www.thewhodidthis.com)",
